#define FOREGROUND 			#D0CCD0
#define BACKGROUND 			#070B14
#define BACKGROUND_LIGHT 	#34373E
#define URGENT	   			#FB3640
#define ACTIVE	   			#43637F

*foreground: FOREGROUND
*background: BACKGROUND

!black		 
*color0:     
*color8:     BACKGROUND
!red
*color1:     #FB5A62
*color9:     URGENT
!green
*color2:     #9BC9C4
*color10:    #85BEB8
!yellow
*color3:     #FBA55A
*color11:    #FB8F36
!blue
*color4:     #657F96
*color12:    ACTIVE
!magenta
*color5:     #FB6970
*color13:    #E5525A
!cyan
*color6:     #768DA1
*color14:    #54718A
!white
*color7:     #D8D5D8
*color15:    FOREGROUND

!################
!# URXVT settings
!################

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt.fading:				10

URxvt*saveLines: 12000

!# Monaco Linux font
URxvt*font: xft:Cousine for Powerline:pixelsize=15
URxvt*boldFont: xft:Cousine for Powerline:pixelsize=15
URxvt*letterSpace: -1

!# scrollbar - true (default) or false
URxvt*scrollBar: true

!# scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: true

!# scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: plain

URxvt.keysym.Control-Up:	\033[1;5A
URxvt.keysym.Control-Down:	\033[1;5B
URxvt.keysym.Control-Left:	\033[1;5D
URxvt.keysym.Control-Right:	\033[1;5C

!!!!!!!!!!!!!!
!Rofi settings
!!!!!!!!!!!!!!

rofi.sidebar-mode: true
rofi.location: 6
rofi.width: 30
rofi.lines: 5
rofi.color-window: FOREGROUND
rofi.color-urgent: URGENT
rofi.color-active: ACTIVE
rofi.color-normal: FOREGROUND